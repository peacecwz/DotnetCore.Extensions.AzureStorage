<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <Authors>Baris Ceviz</Authors>
    <Description>Simple Azure Storage Extensions for .NET Core</Description>
    <Copyright>Copyright @ 2019 Baris Ceviz</Copyright>
    <PackageLicenseUrl>https://github.com/peacecwz/DotnetCore.Extensions.AzureStorage/blob/master/LICENSE</PackageLicenseUrl>
    <PackageId>DotnetCore.Extensions.AzureStorage</PackageId>
    <PackageProjectUrl>https://github.com/peacecwz/DotnetCore.Extensions.AzureStorage</PackageProjectUrl>
    <RepositoryUrl>https://github.com/peacecwz/DotnetCore.Extensions.AzureStorage.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>azure,storage</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="2.2.0" />
    <PackageReference Include="WindowsAzure.Storage" Version="9.3.3" />
  </ItemGroup>

</Project>
